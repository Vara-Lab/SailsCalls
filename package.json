{
  "name": "sailscalls",
  "version": "2.4.6",
  "description": "Typescript library for working with Sails programs",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "gear",
    "sails",
    "sailscalls"
  ],
  "main": "./lib/cjs/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "lib/*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/cjs/index.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "cleanlib": "rm -rfv lib",
    "build:types": "tsc -p tsconfig.types.json",
    "build": "npx rollup --config rollup.config.js && npm run postbuild",
    "postbuild": "node scripts/cjs-dts-bridge.mjs",
    "test": "jest --detectOpenHandles",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@gear-js/api": "0.44.2",
    "@polkadot/api": "16.4.9",
    "decimal.js": "^10.6.0",
    "sails-js": "0.5.1",
    "sails-js-parser": "0.5.1"
  },
  "peerDependencies": {
    "@gear-js/api": "0.44.2",
    "@polkadot/api": "16.4.9",
    "decimal.js": "^10.5.0",
    "sails-js": "0.5.1",
    "sails-js-parser": "0.5.1"
  },
  "devDependencies": {
    "@jest/types": "30.2.0",
    "@polkadot/keyring": "^13.5.7",
    "@rollup/plugin-commonjs": "^28.0.9",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "12.1.4",
    "@types/decimal.js": "0.0.32",
    "@types/jest": "30.0.0",
    "crypto-js": "^4.2.0",
    "jest": "30.2.0",
    "rollup": "^4.52.5",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "lib"
  ]
}
